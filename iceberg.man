$ ./iceberg <command> [options]

GLOBAL OPTIONS

	--no-hook
	--config <config path>

COMMANDS & SPECIFIC OPTIONS

	generate
		--file <file path> || --name <post name>
		[--output <output path]

EXAMPLE USAGE

	./iceberg \
	generate \
	--name hello-world \
	--output greeting-world.html \
	--no-hook \
	--config deploy.ini \
